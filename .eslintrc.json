{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "prettier",
    "plugin:react/recommended"
  ],
  
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef":"off",
    "no-unused-vars":"off",
    "react/prop-types": "off",
    "object-shorthand":[0,"never"]
  }
}